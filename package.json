{
  "name": "currency-rate-app",
  "version": "1.0.0",
  "description": "application that provides USD to UAH exchange rate",
  "main": "dist/index.js",
  "scripts": {
    "test": "test",
    "build": "tsc --build",
    "start": "ts-node src/index.ts",
    "migrate": "node-pg-migrate -m src/db/migrations -j ts",
    "migrate-up": "ts-node --transpileOnly ./src/db/migrate-up.ts",
    "migrate-down": "ts-node --transpileOnly ./src/db/migrate-down.ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "currency"
  ],
  "author": "Iryna Kopyl",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "node-pg-migrate": "^7.3.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "4.42.0",
    "sequelize-typescript": "0.6.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1"
  },
  "dependencies": {
    "googleapis": "^137.1.0",
    "prettier": "^3.2.5"
  }
}
